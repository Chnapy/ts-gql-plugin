{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo",
    "outDir": "./node_modules/.cache/dist",
    "moduleResolution": "node",
    "plugins": [
      {
        "name": "ts-gql-plugin",
        "logLevel": "debug",
        "projectNameRegex": "([A-Z][a-z]*)"
      }
    ]
  },
  "include": ["index.ts"]
}
