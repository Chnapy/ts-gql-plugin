{
  "name": "ts-gql-plugin",
  "version": "0.0.0",
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "author": {
    "name": "Richard Haddad"
  },
  "scripts": {
    "c:format": "prettier -w .",
    "c:lint": "eslint . --cache --cache-location node_modules/.cache/.eslintcache",
    "c:lint:fix": "yarn c:lint --fix",
    "c:type": "tsc --noEmit",
    "c:test": "yarn c:test:watch --run",
    "c:test:watch": "vitest --outputTruncateLength=9999 --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@graphql-codegen/core": "2.5.1",
    "@graphql-codegen/typescript": "2.4.10",
    "@graphql-codegen/typescript-operations": "2.3.7",
    "deasync": "0.1.26"
  },
  "peerDependencies": {
    "graphql": "16.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@types/deasync": "0.1.2",
    "@types/node": "17.0.23",
    "@types/prettier": "2.6.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "c8": "7.11.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "graphql": "16.5.0",
    "prettier": "2.6.2",
    "typescript": "4.6.3",
    "vitest": "0.9.4"
  }
}
