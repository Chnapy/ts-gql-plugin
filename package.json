{
  "name": "ts-gql-plugin",
  "version": "1.3.2",
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "main": "./dist/plugin.js",
  "exports": {
    ".": "./dist/plugin.js",
    "./tools": "./dist/tools.js"
  },
  "files": [
    "dist"
  ],
  "description": "TypeScript Language Service Plugin for GraphQL DocumentNode typing",
  "author": {
    "name": "Richard Haddad"
  },
  "keywords": [
    "typescript",
    "graphql",
    "language service"
  ],
  "homepage": "https://github.com/chnapy/ts-gql-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/chnapy/ts-gql-plugin"
  },
  "bugs": {
    "url": "https://github.com/chnapy/ts-gql-plugin/issues"
  },
  "scripts": {
    "c:format": "prettier -w .",
    "c:lint": "eslint . --cache --cache-location node_modules/.cache/.eslintcache",
    "c:lint:fix": "yarn c:lint --fix",
    "c:type": "tsc --noEmit",
    "c:test": "yarn c:test:watch --run",
    "c:test:watch": "vitest --outputTruncateLength=9999 --passWithNoTests",
    "build": "tsc -p tsconfig.build.json",
    "c:type-gql": "ts-node tsc-ls/tsc-ls.ts -p ./example/tsconfig.json"
  },
  "dependencies": {
    "@graphql-codegen/core": "2.5.1",
    "@graphql-codegen/typescript": "2.4.10",
    "@graphql-codegen/typescript-operations": "2.3.7",
    "deasync": "0.1.26",
    "graphql-config": "4.3.1"
  },
  "peerDependencies": {
    "graphql": ">= 16",
    "graphql-tag": ">= 2"
  },
  "devDependencies": {
    "@apollo/client": "3.6.7",
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@types/deasync": "0.1.2",
    "@types/node": "17.0.23",
    "@types/prettier": "2.6.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "c8": "7.11.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "graphql": "16.5.0",
    "graphql-tag": "2.12.6",
    "prettier": "2.6.2",
    "ts-node": "10.8.1",
    "tslib": "2.4.0",
    "typescript": "4.7.3",
    "vite": "2.9.10",
    "vitest": "0.9.4"
  }
}
